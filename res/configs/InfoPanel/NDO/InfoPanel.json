{
    "enabled": true,
    "aliveOnly": false,
    "altKey": ["KEY_LALT"],
    "compareValues": {
        "equal": {
            "color": "#FFFFFF",
            "delim": "="
        },
        "lessThan": {
            "color": "#00FF00",
            "delim": "&lt;"
        },
        "moreThan": {
            "color": "#FF0000",
            "delim": "&gt;"
        }
    },
    "format": "<font color='#FFFFFF'><textformat tabstops='[5,150,214]'><font size='0'>.</font><tab><font color='#FF9900'>{{vehicle_name}}</font><tab>Reload:<tab>{{gun_reload_equip}} sec</textformat></b><textformat tabstops='[5,54,150,214,246,279]'><font size='0'>.</font><tab>View:<tab>{{vision_radius}} mt<tab>Shell Type:<tab>{{shell_type_1}}<tab>{{shell_type_2}}<tab>{{shell_type_3}}</textformat></b><textformat tabstops='[5,54,80,105,150,214,246,279]'><font size='0'>.</font><tab>Torret:<tab>{{armor_turret_front}}<tab>{{armor_turret_side}}<tab>{{armor_turret_back}}<tab>Av. Damage:<tab>{{shell_damage_1}}<tab>{{shell_damage_2}}<tab>{{shell_damage_3}}</textformat></b><textformat tabstops='[5,54,80,105,150,214,246,279]'><font size='0'>.</font><tab>Chassis:<tab>{{armor_hull_front}}<tab>{{armor_hull_side}}<tab>{{armor_hull_back}}<tab>Piercing:<tab>{{shell_power_1}}<tab>{{shell_power_2}}<tab>{{shell_power_3}}</textformat></font>",
    "showFor": 0,
    "delay": 5,
    "textLock": false,
    "textPosition": [632.0, 577.0],
    "textShadow": {
        "alpha": 0.8,
        "angle": 90,
        "blurX": 2,
        "blurY": 2,
        "color": "#000000",
        "distance": 0,
        "quality": 2,
        "strength": 2
    },
    "textStyle": {
        "align": "left",
        "font": "$TitleFont",
        "size": 15
    }
}