{
    "aliveOnly": false,
    "altKey": ["KEY_LALT"],
	    "compareValues": {
        "equal": {
            "color": "#FFFFFF",
            "delim": "="
        },
        "lessThan": {
            "color": "#00FF00",
            "delim": "&lt;"
        },
        "moreThan": {
            "color": "#FF0000",
            "delim": "&gt;"
        }
    },
    "delay": 5,
	"enabled": true,
	"format": "<font face='$FieldFont' size='14' color='#FFFFFF'><font color='#FF557F'>{{vehicle_name}}</font>\nReload: <font color='#17EB17'>{{gun_reload_equip}}sec.</font>\n<img src='img://gui/maps/icons/vehicle/{{icon_system_name}}.png'>\n<textformat tabstops='[65]'>Weight: <font color='#FF3C38'>{{vehicle_weight}} ton.</font><tab>View: <font color='#002AFF'>{{vision_radius}} mt.</font></textformat>n<textformat tabstops='[65,105,145]'>Cassis:<tab>{{armor_hull_front}}<tab>{{armor_hull_side}}<tab>{{armor_hull_back}}</textformat>\n<textformat tabstops='[65,105,145]'>Torret:<tab>{{armor_turret_front}}<tab>{{armor_turret_side}}<tab>{{armor_turret_back}}</textformat>\n<textformat tabstops='[65,105,145]'>Shell Type:<tab><font color='#FF7F00'>{{shell_type_1}}</font><tab><font color='#FFD400'>{{shell_type_2}}</font><tab><font color='#FFAA55'>{{shell_type_3}}</font></textformat>\n<textformat tabstops='[65,105,145]'>Piercing:<tab>{{shell_power_1}}<tab>{{shell_power_2}}<tab>{{shell_power_3}}</textformat>\n<textformat tabstops='[65,105,145]'>Av. Damage:<tab>{{shell_damage_1}}<tab>{{shell_damage_2}}<tab>{{shell_damage_3}}</textformat></font>",
	"showFor": 0,
    "textLock": false,
    "textPosition": {
        "x": 399.0,
        "y": 835.0
    },
    "textShadow": {
        "alpha": 0.8,
        "angle": 90,
        "blurX": 2,
        "blurY": 2,
        "color": "#000000",
        "distance": 0,
        "enabled": true,
        "quality": 2,
        "strength": 2
    },
    "textStyle": {
        "align": "center",
        "font": "$TitleFont",
        "size": 14
    }
}